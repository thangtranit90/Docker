{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "scriptUri": {
            "value": "https://<mystorageaccount>.blob.core.windows.net/scripts/webserver.sh"
        },
        "StorageAccountName": {
            "value": "<mystorageaccount name>"
        },
        "StorageAccountKey": {
            "value": "<mystorageaccount key>"
        },
        "commandToExecute": {
            "value": "sh webserver.sh"
        }
    }
}